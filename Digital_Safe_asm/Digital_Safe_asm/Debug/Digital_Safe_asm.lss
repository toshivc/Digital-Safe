                                 ;***********************************************************
                                 ; Define variables
                                 .def TEMP = R16			; 
                                 .equ SP = 0xDF			; 
                                 
                                 ; Rename column masks
                                 .equ col1 = 0xEF		; Column 1 including 1, 4, 7, *
                                 .equ col2 = 0xDF		; Column 2 including 2, 5, 8, 0
                                 .equ col3 = 0xBF		; Column 3 including 3, 6, 9, #
                                 .equ col4 = 0x7F		; Column 4 inlcuding A, B, C, D
                                 
                                 ; Rename keys
                                 .equ key0  = 0xD7
                                 .equ key1  = 0xEE
                                 .equ key2  = 0xDE
                                 .equ key3  = 0xBE
                                 .equ key4  = 0xED
                                 .equ key5  = 0xDD
                                 .equ key6  = 0xBD
                                 .equ key7  = 0xEB
                                 .equ key8  = 0xDB
                                 .equ key9  = 0xBB
                                 .equ key10 = 0x7E		; A key
                                 .equ key11 = 0x7D		; B key
                                 .equ key12 = 0X7B		; C key
                                 .equ key13 = 0X77		; D key
                                 .equ key14 = 0XE7		; * key
                                 .equ key15 = 0XB7		; # key
                                 
                                 ;***********************************************************
                                 
                                 
                                 ;***********************************************************
                                 ; Define here Reset and interrupt vectors, if any
                                 reset:
000000 c010                         rjmp start
000001 9518                         reti      ; Addr $01
000002 9518                         reti      ; Addr $02
000003 9518                         reti      ; Addr $03
000004 9518                         reti      ; Addr $04
000005 9518                         reti      ; Addr $05
000006 9518                         reti      ; Addr $06        Use 'rjmp myVector'
000007 9518                         reti      ; Addr $07        to define a interrupt vector
000008 9518                         reti      ; Addr $08
000009 9518                         reti      ; Addr $09
00000a 9518                         reti      ; Addr $0A
00000b 9518                         reti      ; Addr $0B        This is just an example
00000c 9518                         reti      ; Addr $0C        Not all MCUs have the same
00000d 9518                         reti      ; Addr $0D        number of interrupt vectors
00000e 9518                         reti      ; Addr $0E
00000f 9518                         reti      ; Addr $0F
000010 9518                         reti      ; Addr $10
                                 
                                 ;***********************************************************
                                 
                                 
                                 ;***********************************************************
                                 ; Start program
                                 start:
000011 ed0f                      		LDI TEMP, SP		; Init Stack Pointer
000012 bf0d                      		OUT 0x3D, TEMP		; Load TEMP value into 0x3D
                                 
000013 940e 001a                 		CALL Initialise		; Go to initialise subroutine
                                 
                                 loop: 
000015 940e 0025                 		CALL ReadOne
000017 940e 0093                 		CALL Display
000019 cffb                      		RJMP loop
                                 
                                 
                                 
                                 ;***********************************************************
                                 
                                 
                                 ;***********************************************************
                                 ; Initialise PORT B, PORT C and PORT C timers
                                 Initialise:	
                                 		; Initalise Port B
00001a ef0f                      		LDI TEMP, 0xFF		; 0b11111111
00001b bb07                      		OUT DDRB, TEMP		; Outputs to DDRB (all outputs)
                                 
00001c e000                      		LDI TEMP, 0x00		; 0b00000000
00001d bb08                      		OUT PORTB, TEMP		; Turns all LEDs off
                                 
                                 		; Initialise Port C
00001e ef00                      		LDI TEMP, 0xF0		; 0b11110000
00001f bb04                      		OUT DDRC, TEMP		; Outptus to DDRC (sets rows to inputs and columns to outputs)
                                 
000020 e00f                      		LDI TEMP, 0x0F		; 0b00001111
000021 bb05                      		OUT PORTC, TEMP		; Enable pull up resistors on the row inptus
                                 
000022 ef0f                      		LDI TEMP, 0xFF		; 0b11111111
000023 bb05                      		OUT PORTC, TEMP		; Set columns to all on (idle state)
                                 
                                 		; Timers
                                 
                                 
000024 9508                      		RET
                                 
                                 ;***********************************************************
                                 
                                 
                                 ;***********************************************************
                                 ; ReadOne function 
                                 
                                 ReadOne:
                                 	Column1:
000025 ee0f                      	    LDI	temp, col1		; Assign 1st column to temp
000026 bb05                      		OUT portC, temp		; Put temp into portC
000027 940e 0095                 		CALL Delay
                                 
000029 b303                      		IN temp, pinC		; Read PINC values (0x13) and put into temp
00002a 3e0f                      		CPI temp, col1		; Compare pinC value with col1
00002b f0a1                      		BREQ Column2		; Go to Column2 if equal (key has not been pressed in col1)
                                 		
00002c 3e0e                      		CPI temp, key1		; check for key1
00002d f419                      		BRNE CheckKey4		; If not equal, check for key4
00002e e001                      		LDI temp, 0x01		; If equal, temp = 1
00002f bb08                      		OUT portB, temp		; output key to LED
000030 9508                      		RET					; Return back
                                 
                                 	CheckKey4:	     	; 
000031 3e0d                      		CPI temp, key4		; check for key4
000032 f419                      		BRNE CheckKey7		; If not equal, check for key7
000033 e004                      		LDI temp, 0x04		; If equal, temp = 4
000034 bb08                      		OUT portB, temp		; output key to LED
000035 9508                      		RET					; Reutrn back
                                 
                                 	CheckKey7:	     	; 
000036 3e0b                      		CPI temp, key7		; check for key7
000037 f419                      		BRNE CheckKey14		; If not equal, check for key10
000038 e007                      		LDI temp, 0x07		; If equal, temp = 7
000039 bb08                      		OUT portB, temp		; output key to LED
00003a 9508                      		RET					; Reutrn back
                                 
                                 	CheckKey14:	     	; 
00003b 3e07                      		CPI temp, key14		; check for key4
00003c f419                      		BRNE Column2		; If not equal, check for Column2
00003d ef00                      		LDI temp, 0xF0		; If equal, temp = 10
00003e bb08                      		OUT portB, temp		; output key to LED
00003f 9508                      		RET					; Reutrn back
                                 
                                 	Column2:			
000040 ed0f                      	    LDI	temp, col2		; Assign 2nd column to temp
000041 bb05                      		OUT portC, temp		; Put temp into portC
000042 940e 0095                 		CALL Delay
                                 
000044 b303                      		IN temp, pinC		; Read PINC values (0x13) and put into temp
000045 3d0f                      		CPI temp, col2		; Compare pinC value with col2
000046 f0a1                      		BREQ Column3		; Go to Column3 if equal (key has not been pressed in col2)
                                 		
000047 3d0e                      		CPI temp, key2		; check for key2
000048 f419                      		BRNE CheckKey5		; If not equal, check for key5
000049 e002                      		LDI temp, 0x02		; If equal, temp = 2
00004a bb08                      		OUT portB, temp		; output key to LED
00004b 9508                      		RET					; Return back
                                 
                                 	CheckKey5:	     	; 
00004c 3d0d                      		CPI temp, key5		; check for key5
00004d f419                      		BRNE CheckKey8		; If not equal, check for key8
00004e e005                      		LDI temp, 0x05		; If equal, temp = 5
00004f bb08                      		OUT portB, temp		; output key to LED
000050 9508                      		RET					; Reutrn back
                                 
